{
  "name": "phpmd",
  "description": "VS Code extension for php, using phpmd.",
  "engines": {
    "node": "*"
  },
  "version": "1.0.0",
  "author": "Yang Lin",
  "license": "SEE LICENSE IN LICENSE.md",
  "dependencies": {
    "vscode-languageserver": "^3.0.4"
  },
  "devDependencies": {
    "@types/node": "^6.0.52",
    "typescript": "^2.1.5"
  },
  "scripts": {
    "compile": "${workspaceRoot}node_modules/.bin/installServerIntoExtension ../client ./package.json ./tsconfig.json && tsc -p .",
    "watch": "${workspaceRoot}node_modules/.bin/installServerIntoExtension ../client ./package.json ./tsconfig.json && tsc --watch -p ."
  }
}
